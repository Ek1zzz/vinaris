{
  "name": "vinaris-backend",
  "version": "1.0.0",
  "description": "VINaris VIN checking service backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "node database/migrate.js",
    "backup": "cd database && ./backup.sh",
    "test": "jest",
    "seed": "node database/db-helper.js test",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "security:check": "node scripts/security-check.js",
    "security:scan": "node scripts/vulnerability-scan.js",
    "lint:security": "eslint . --ext .js --config .eslintrc.security.js",
    "test:security": "jest --testPathPattern=security"
  },
  "keywords": [
    "vin",
    "vehicle",
    "history",
    "carfax",
    "autocheck"
  ],
  "author": "VINaris Team",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "html-pdf-node": "^1.0.8",
    "isomorphic-dompurify": "^2.26.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "puppeteer": "^24.17.0",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.6",
    "stripe": "^12.17.0",
    "uuid": "^9.0.0",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/vinaris.git"
  }
}
